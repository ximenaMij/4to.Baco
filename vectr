using System;

namespace vectr
{
    class Program
    {
        static void Main(string[] args)
        {
            char[] dato = new char[10];

            dato[0] = 'a';

            for(int i=0; i<=9; i++)
            {
                Console.WriteLine("i: " + i);
                Console.Write("Ingrese una letra: ");
                dato[i] = char.Parse(Console.ReadLine());
            }
            Console.ReadKey();
        }
    }
}
